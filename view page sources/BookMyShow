		<script type="text/javascript">
			function getParameterByName(name, url = window.location.href) {
				try {
					    name = name.replace(/[\[\]]/g, '\\$&');
					    var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
					        results = regex.exec(url);
					    if (!results) return null;
					    if (!results[2]) return '';
					    return decodeURIComponent(results[2].replace(/\+/g, ' '));
				} catch(e) {
					console.log('Catch error in getParameterByName');
				}
			}
		</script>
		<script type="text/javascript">
			$(function() {
				/**
				 * This calls CANCELTRANS on click of back button.
				 * Disabling this functionality for merch flow.
				*/
			    if (window.history && window.history.pushState  && !pay.isMerch()) {

			    	if(!getParameterByName('reload')) {
			    		var blnReload = "reload=true";
			    		blnReload = window.location.search ? '&'+blnReload : '?'+blnReload;
			    		window.history.pushState('', null, document.URL+blnReload);
			    	}

			    	$(window).on('popstate', function() {
			        	if (!/Edge/.test(navigator.userAgent) && window.location.hash == '' && location.search.indexOf('lrRedirect') == -1) {
								BMS.Misc.fnBusy(true);
								pay.fnCanTr();
						}
			        });
			    }
			});
